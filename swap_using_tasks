module tb;
  task swap(input bit [3:0]a,b);
    bit [3:0]temp;
    temp=a;
    a=b;
    b=temp;
    $display("a=%0d b=%0d",a,b);
  endtask
  bit [3:0]a;
  bit [3:0]b;
  
    initial begin
      a=7;
      b=3;
      $display("a=%0d b=%0d",a,b);
      swap(a,b);
    end
    endmodule
