// Code your testbench here
// or browse Examples
class lifo;
  int intQ[$];
  function void put(int a);
    intQ.push_back(a);
  endfunction
  function void get(output int a);
   a=intQ.pop_back();
  endfunction
endclass

module tb;
  lifo stack=new();
  int num;
  initial begin
    repeat(5) begin
      num=$urandom_range(10,30);
      stack.put(num);
      $display("num=%0d",num);
    end
    repeat(5) begin
      stack.get(num);
      $display("num=%0d",num);
  end
  end
 endmodule 
      
      
    
  
